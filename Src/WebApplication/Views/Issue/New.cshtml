@using Service.ViewModels.Project
@model object

@{
	ViewBag.Title = "Dashboard";
	Layout = "~/Views/Shared/Layout.cshtml";
}

@{
	var projects = (List<ProjectListViewModel>)ViewBag.Projects;
	var defaultProjectId = projects.FirstOrDefault()?.Id;
}

<script type="text/javascript">
	window.app = {};
	window.app.projects = [];
	window.app.projects = @(Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(projects)));
	window.app.defaultProjectId = @defaultProjectId;
</script>

<div id="app-issue-create-root"></div>
@Html.Partial("_ReactJs")